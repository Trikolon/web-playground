  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.2",
    "@types/node": "^24.2.0",
    "dedent": "^1.6.0",
    "glob": "^11.0.3",
    "vite": "^7.1.0",
    "wrangler": "^4.28.1"
  },
  "packageManager": "pnpm@10.14.0"
}
